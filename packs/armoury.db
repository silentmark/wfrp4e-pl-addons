{"name":"Futro Luźne","type":"armour","flags":{"_sheetTab":"details","core":{"sourceId":"Compendium.wfrp4e-pl.armoury.QhAUCEXh9rS430bY"}},"img":"modules/wfrp4e-pl/icons/furarmour.webp","system":{"description":{"type":"String","label":"Description","value":"<p>Futra jako ochrony przed bronią stosowane są od zarania dziejów. Ich naturalną skuteczność docenia każdy, kto próbował walczyć z niedzwiedziem i poległ. Każdy element zbroi futrzanej ze względu na swój ciężar posiada wadę „nieporęczny”, która jest niemożliwa do usunięcia.</p>"},"gmdescription":{"type":"String","label":"Description","value":""},"quantity":{"type":"Number","label":"Quantity","value":1},"encumbrance":{"type":"Number","label":"Encumbrance","value":2},"price":{"type":"String","label":"Price","gc":"0","ss":"10","bp":"0"},"availability":{"type":"String","label":"Availability","value":"common"},"location":{"type":"Number","label":"Location","value":0},"damageToItem":{"type":"Number","value":0,"shield":0},"worn":{"type":"Boolean","label":"Worn","value":false},"armorType":{"type":"String","label":"Armour Type","value":"light"},"penalty":{"type":"String","label":"Penalty","value":""},"qualities":{"type":"String","label":"Qualities","value":[]},"flaws":{"type":"String","label":"Flaws","value":[{"name":"partial","value":null},{"name":"bulky","value":null}]},"special":{"type":"String","label":"Special","value":"Warstwa Zewnętrzna, Futro"},"AP":{"head":1,"lArm":0,"rArm":0,"lLeg":0,"rLeg":0,"body":1},"APdamage":{"head":0,"lArm":0,"rArm":0,"lLeg":0,"rLeg":0,"body":0},"maxAP":{"head":0,"lArm":2,"rArm":2,"lLeg":0,"rLeg":0,"body":0},"source":{"type":"String","label":"Source"}},"effects":[],"_stats":{"systemId":"wfrp4e","systemVersion":"6.3.0","coreVersion":"10.291","createdTime":1675639492194,"modifiedTime":1675815874969,"lastModifiedBy":"Czz7IJA9DEa5LD9E"},"_id":"QhAUCEXh9rS430bY","folder":null,"sort":0,"ownership":{"default":0,"Czz7IJA9DEa5LD9E":3}}
